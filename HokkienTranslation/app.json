{
  "expo": {
    "name": "HokkienTranslation",
    "slug": "HokkienTranslation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hokkientranslation.hokkientranslation",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.hokkientranslation.hokkientranslation",
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/close-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e58da73-63c9-4f14-a312-6296202406e0"
      }
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow audio playback"
        }
      ]
    ]
  }
}
